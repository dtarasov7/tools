- name: Обрабатываем список текущих политик пользователей
  ansible.builtin.set_fact:
    current_policies_dict: >-
      {{
        dict(
          current_policies.results | 
          map(attribute='item.name') | 
          list | 
          zip(
            current_policies.results | 
            map(attribute='stdout_lines') | 
            list
          )
        )
      }}